{
 "stage": 0,
  "optional": [
    "es7.decorators",
    "es7.objectRestSpread"
  ],
  "env": {
    "start": {
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [
            {
              "transform": "react-transform-hmr", //Hot module replacement
              "imports": ["react"],
              "locals": ["module"]
            }
          ]
        }
      }
    }
  }
}
